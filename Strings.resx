  <data name="MagGrabPage" xml:space="preserve">
    <value>&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"&gt;
&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;
&lt;head&gt;
	&lt;meta http-equiv="Content-Type" content="text/html; charset=utf-8" /&gt;
	&lt;title&gt;Magnet Grabber v1.0&lt;/title&gt;

	&lt;script type="text/javascript" language="javascript"&gt;
	String.prototype.trim = function() {
		return this.replace(/^\s+|\s+$/g, "");
	}

	function clear_box() {
		if (document.getElementById('hash_txt').value == 'paste torrent hash here') {
			document.getElementById('hash_txt').value = '';
		}
	}

	function clear_list() {
		if (document.getElementById('tracker_list').value == 'paste trackers here (optional)') {
			document.getElementById('tracker_list').value = '';
		}
	}

	function get_torrent() {
		var tl_area = document.getElementById('tracker_list');
		var magnet_uri = 'magnet:?xt=urn:btih:';
		magnet_uri += document.getElementById('hash_txt').value.trim();

		if (tl_area.value != 'paste trackers here (optional)' &amp;&amp; tl_area.value != '') {
			var tracker_list = tl_area.value.split("\n");
			var tl_count = tracker_list.length;
			var counter = 0;
			for (counter=0;counter&lt;tl_count;counter++) {
				magnet_uri += '&amp;tr=' + tracker_list[counter];
			}
		}
		
		window.location = magnet_uri;
	}
	&lt;/script&gt;
&lt;/head&gt;
&lt;body&gt;

	&lt;h1&gt;Enter the torrent hash:&lt;/h1&gt;
	&lt;input id="hash_txt" type="text" style="width:300px" value="paste torrent hash here" onFocus="clear_box();" /&gt; 
	&lt;input type="button" value="get torrent" onClick="get_torrent();" /&gt;
	&lt;br /&gt;&lt;br /&gt;
	&lt;textarea id="tracker_list" cols="45" rows="5" style="width:300px" onFocus="clear_list();" &gt;paste trackers here (optional)&lt;/textarea&gt;

&lt;/body&gt;
&lt;/html&gt;</value>
